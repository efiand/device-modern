<footer class="page-footer {{ cssmix }}">
	<div class="centerer page-footer__inner">
		{% set cssMix = "page-footer__logo" %}
		{% include getComponentPath("logo") %}
		{{ address }}

		<a class="page-footer-dev" href="{{ developer.href }}">
			{% with developer.logo %}
				<img src="{{ src }}" alt="{{ alt }}" width="{{ width }}" height="{{ height }}">
			{% endwith %}
		</a>
	</div>
</footer>
